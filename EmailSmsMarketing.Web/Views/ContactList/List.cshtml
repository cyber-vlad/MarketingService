@using EmailSmsMarketing.Web.Models.ContactLists.List;
@model ContactListsViewModel
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card" style="margin-top: 50px; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);">
    <div class="card-body">
        <div class="edit-title">
            @{var contactId = Model.ID;}
            <h4 style="color: #1e1e1e; margin-left: 5px; margin-right: 10px;">
                @Model.Name
             </h4>
            <div id="editTitleButton" data-contact-id="@contactId"></div>
        </div>
      
        <div id="contactListTable" class="col-lg-12"></div>
    </div>
</div>

<script src="~/js/ISESM.js" asp-append-version="true"></script>
<script src="~/js/ISESMDevextreme.js" asp-append-version="true"></script>
<script src="~/js/ContactsList/ContactList/List/ContactListDevexpress.js" asp-append-version="true"></script>
<script src="~/js/ContactsList/ContactList/ChangeTitle/ChangeTitleContactListDevexpress.js" asp-append-version="true"></script>
